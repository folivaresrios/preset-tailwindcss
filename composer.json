{
    "name": "folivaresrios/preset-tailwindcss",
    "description": "Laravel 5.5 frontend preset for Tailwind CSS",
    "keywords": ["laravel", "preset", "tailwindcss"],
    "license": "MIT",
    "require": {
        "laravel/framework": "5.5.*|5.6.*"
    },
    "autoload": {
        "psr-4": {
            "KissDev\\TailwindCssPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KissDev\\TailwindCssPreset\\TailwindCssPresetServiceProvider"
            ]
        }
    }
}
